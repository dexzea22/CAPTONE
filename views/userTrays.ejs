      // Add event listener for keydown event
      document.getElementById('menuSearchInput').addEventListener('keydown', function(event) {
        // Check if Enter key is pressed (key code 13)
        if (event.keyCode === 13) {
          performSearch();
        }
      });
    
      function performSearch() {
        // Get the value from the search input
        var searchQuery = document.getElementById('menuSearchInput').value.toLowerCase();
    
        // Perform your search logic here
        // For demonstration, you can hide/show cards based on the search query
        cards.forEach(function(card) {
          var cardTitle = card.querySelector('h4').innerText.toLowerCase();
          if (cardTitle.includes(searchQuery)) {
            card.style.display = 'block';
          } else {
            card.style.display = 'none';
          }
        });
      }
    </script>
