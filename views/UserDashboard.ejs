  function updateCart() {
    var cartList = document.getElementById("modal-cart-items");
    var totalAmount = document.getElementById("total-amount");
    cartList.innerHTML = "";

    var totalPrice = 0;

    for (var i = 0; i < cartItems.length; i++) {
